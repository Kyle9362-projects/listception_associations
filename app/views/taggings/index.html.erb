<div class="page-header">
  <h1>
    Taggings
    <a href="/taggings/new" class="btn btn-lg btn-default">New Tagging</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Tag</th>
        <th>Card</th>
        <th>Actions</th>
      </tr>

      <% @taggings.each do |tagging| %>
      <tr>
        <td><%= tagging.tag_id %></td>
        <td><%= tagging.card_id %></td>
        <td>
          <a href="/taggings/<%= tagging.id %>" class="btn btn-default">Show</a>
          <a href="/taggings/<%= tagging.id %>/edit" class="btn btn-default">Edit</a>
          <a href="/delete_tagging/<%= tagging.id %>" class="btn btn-default" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
